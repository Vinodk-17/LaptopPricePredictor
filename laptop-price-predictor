    "# making the ppi column for the laptop data using the formula \n",
    "# ppi = sqrt(x)+sqt(y)/inches\n",
    "df['ppi'] = (((df['X_res']**2)+(df['Y_res']**2))**0.5/df['Inches']).astype('float')"
   ]
